@prefix : <http://data.vlaanderen.be/ns#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dvldtp: <http://data.vlaanderen.be/ns/datatypes#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://data.vlaanderen.be/ns>
  rdf:type owl:Ontology ;
  owl:imports <http://data.vlaanderen.be/ns/datatypes> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
:CodeType
  rdf:type owl:Class ;
  rdfs:comment "Een entiteit die een type heeft dat een bepaalde codelijst volgt."^^xsd:string ;
  rdfs:label "CodeType"^^xsd:string ;
.
:KernObject
  rdf:type owl:Class ;
  rdfs:comment "KernObject Data Vlaanderen."^^xsd:string ;
  rdfs:label "KernObject"^^xsd:string ;
.
:alternatieveIdentificator
  rdf:type owl:ObjectProperty ;
  rdfs:comment "Alternatieve identificator van de resource."^^xsd:string ;
  rdfs:label "alternatieve identificator"^^xsd:string ;
  rdfs:range dvldtp:Identificator ;
  rdfs:subPropertyOf <http://www.w3.org/ns/adms#identifier> ;
.
:geldigheidsPeriode
  rdf:type owl:ObjectProperty ;
  rdfs:domain :KernObject ;
  rdfs:label "geldigheidsPeriode"^^xsd:string ;
  rdfs:range dvldtp:Periode ;
  rdfs:subPropertyOf dcterms:temporal ;
.
:herkomst
  rdf:type owl:ObjectProperty ;
  rdfs:comment "Herkomst van de resource."^^xsd:string ;
  rdfs:label "herkomst"^^xsd:string ;
  rdfs:range <http://www.w3.org/ns/prov#Entity> ;
  rdfs:subPropertyOf <http://www.w3.org/ns/prov#entity> ;
.
:identificator
  rdf:type owl:ObjectProperty ;
  rdfs:comment "Identificator van de resource."^^xsd:string ;
  rdfs:label "identificator"^^xsd:string ;
  rdfs:range dvldtp:Identificator ;
  rdfs:subPropertyOf <http://www.w3.org/ns/adms#identifier> ;
.
:type
  rdf:type owl:ObjectProperty ;
  rdfs:comment "het type van de resource volgens een codelijst."^^xsd:string ;
  rdfs:label "type"^^xsd:string ;
  rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
  rdfs:subPropertyOf dcterms:type ;
.
